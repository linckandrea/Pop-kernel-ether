#include "msm8992-memory-pstore.dtsi"
#include "PMIC-PMI8994_nbq_charger_fg_dvt_pvt.dtsi"

/ {
	aliases {
		i2c1 = &i2c_1;
	};
};

&soc {
	tlmm_pinmux: pinctrl@fd510000 {
		pmx_i2c_10 {
			qcom,pins = <&gp 55>, <&gp 56>; /* SDA, SCL */
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <4>;
			label = "pmx_i2c_10";

			i2c_10_active: i2c_10_active {
				drive-strength = <2>;
				bias-disable;
			};

			i2c_10_sleep: i2c_10_sleep {
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	i2c_1: i2c@f9966000 {
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0xf9966000 0x1000>;
		interrupt-names = "qup_irq";
		interrupts = <0 104 0>;
		qcom,clk-freq-out = <400000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp2_ahb_clk>,
			 <&clock_gcc clk_gcc_blsp2_qup4_i2c_apps_clk>;

		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_10_active>;
		pinctrl-1 = <&i2c_10_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		dmas = <&dma_blsp2 18 64 0x20000020 0x20>,
			<&dma_blsp2 19 32 0x20000020 0x20>;
		dma-names = "tx", "rx";
		qcom,master-id = <84>;
	};

	i2c@f9967000 { /* BLSP2 QUP5 */
		hd3ss3220@67 {
			compatible = "ti,hd3ss3220";
			reg = <0x67>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <9 0x2>;
			hd3ss3220vdd-supply = <&pm8994_l19>;
			usb_redriver-supply = <&pm8994_l31>;
			pinctrl-names = "pmx_ts_active", "pmx_ts_suspend";
			pinctrl-0 = <&ts_active>;
			pinctrl-1 = <&ts_suspend>;
			qcom,clk-freq-out = <100000>;
			qcom,clk-freq-in  = <19200000>;
			clock-names = "iface_clk", "core_clk";
			clocks = <&clock_gcc clk_gcc_blsp2_ahb_clk>,
				<&clock_gcc clk_gcc_blsp2_qup5_i2c_apps_clk>;
			ti-hd3ss3220,irq-gpio = <&msm_gpio 9 0x00>;
		};
	};
};

#include "msm8992_reset_key_nbq_evt.dtsi"
#include "nbq-wled.dtsi"
#include "nbq_LP5523.dtsi"

&spi_0 {
	status = "disabled";
};

#include "nbq-haptics.dtsi"
#include "nbq-fpc-fingerprint.dtsi"
